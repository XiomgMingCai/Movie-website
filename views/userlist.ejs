<%- include('layout/head') %>
<%- include('layout/header') %>
<%- include('layout/signup_.ejs') %>
<div class="container">
    <div class="row">
        <table class="table table-hover table-bordered">
            <thead>
            <tr>
                <th>名字</th>
                <th>时间</th>
                <th>查看</th>
                <th>修改</th>
                <th>删除</th>
            </tr>
            <tbody>
            <% users.forEach((item)=>{ %>
            <tr class="item-id-<%= item._id %>">
                <td><%= item.name %></td>
                <td><%= moment(item.meta.updateAt).format('MM/DD/YYYY')%></td>
                <td><a href="../detail/<%= item._id %>" target="_blank">查看</a></td>
                <td><a href="../admin/control/update/<%= item._id %>" target="_blank">修改</a></td>
                <td>
                    <button class="btn btn-danger del" data-id="<%= item._id %>">删除</button>
                </td>
            </tr>
            <% }) %>
            </tbody>
            </thead>
        </table>
    </div>
</div>
<!--<script src="/javascripts/list_Delete_Admin.js"></script>-->
</body>
</html>